{
  "name": "muj",
  "version": "1.0.0",
  "main": "server.js",
  "proxy": "http://localhost:5000/",
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "cd client && yarn start ",
    "dev": "concurrently \"yarn run server\" \"yarn run client\"",
    "postinstall": "cd client && NPM_CONFIG_PRODUCTION=false yarn install && yarn build"
  },
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "concurrently": "^5.0.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-fileupload": "^1.1.6-alpha.6",
    "express-session": "^1.17.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.7.9",
    "multer": "^1.4.2",
    "nodemailer": "^6.3.1",
    "nodemon": "^1.19.4",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "pdf-lib": "^1.2.1",
    "pdf2json": "^1.1.8",
    "pdfkit": "^0.10.0",
    "pdfreader": "^1.0.6",
    "validation": "^0.0.1",
    "validator": "^12.0.0"
  }
}
